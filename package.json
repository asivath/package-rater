{
  "name": "package-rater-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "ECE 461 Project Phase 2",
  "homepage": "https://github.com/asivath/package-rater#readme",
  "bugs": {
    "url": "https://github.com/asivath/package-rater/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asivath/package-rater.git"
  },
  "license": "MIT",
  "author": "Aditya Sivathanu, Kevin Chang, Ellis Selznick",
  "type": "module",
  "workspaces": [
    "app",
    "package-rater-cli",
    "amplify"
  ],
  "scripts": {
    "build": "yarn workspaces foreach -A --parallel run build",
    "dev": "yarn workspace package-rater-app dev",
    "format": "yarn prettier --write app/src package-rater-cli/src amplify '*.json' '.github/workflows/*.yml'",
    "lint": "cd app && yarn lint && cd ../package-rater-cli && yarn lint && cd ../amplify && yarn lint",
    "quality": "yarn format && yarn lint && yarn clean",
    "test": "yarn workspaces foreach -A --parallel run test",
    "test:ci": "yarn workspacse foreach -A --parallel run test:ci",
    "test:coverage": "yarn workspaces foreach -A --parallel run test:coverage",
    "test:coverage-ci": "yarn workspaces foreach -A --parallel run test:coverage-ci",
    "clean": "yarn workspaces foreach -A run clean && rm -rf coverage node_modules",
    "aws": "yarn ampx sandbox"
  },
  "dependencies": {
    "aws-amplify": "^6.6.5"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.5.0",
    "@aws-amplify/backend-cli": "^1.2.9",
    "@aws-amplify/platform-core": "^1.1.0",
    "@aws-sdk/client-amplify": "^3.672.0",
    "@aws-sdk/client-cloudformation": "^3.674.0",
    "@aws-sdk/client-s3": "^3.674.0",
    "@aws-sdk/types": "3.664.0",
    "aws-cdk": "^2",
    "aws-cdk-lib": "^2",
    "constructs": "^10.0.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.11.1",
    "graphql": "^16.9.0",
    "pluralize": "^8.0.0",
    "prettier": "^3.3.3",
    "ts-dedent": "^2.2.0",
    "tsx": "^4.19.1",
    "typescript": "^5.0.0",
    "zod": "^3.23.8"
  },
  "packageManager": "yarn@4.5.0"
}
